{
    "manifest_version" : 3,
    "name" : "HaoChiBlock",
    "version" : "1.0",
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["Content.js"],
            "run_at" : "document_start"
        }
    ],
    "action": {
        "default_popup" : "popup.html",
        "default_icon" : "./images/icon.png"
    },
    "permissions" : [
        "storage"
    ]
}